#TouhouScript[Stage]
#Title[ExRumia]
#Text[ExRumia]
#Music[".\music\Text.mp3"]
#Image[".\img\ExRumia.png"]
#BackGround[Default]
#Player[FREE]
#ScriptVersion[1]

script_stage_main
{
	var start, end;
	
	@Initialize
	{
		start = 100;
		end = 200;
	}
	
	@MainLoop
	{
		var SYS_time;
		if (SYS_time > start && SYS_time < end)
		{
			var x,y;
			x = getPlayerX();
			y = getPlayerY();
			x += 30;
			++x;
			x = 1, y=2;
			y -= 50 + x;
			makeBullet01(1,x+20, y+20, 2,45);
		}
	}
}